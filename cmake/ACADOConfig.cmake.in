################################################################################
#
# Configurable section
#
################################################################################

#
# Tell the user project where to find our headers, libraries and external
# packages, etc.
#
SET( ACADO_INCLUDE_DIRS "~/catkin_ws/devel/include/acado" )
SET( ACADO_LIBRARY_DIRS "~/catkin_ws/build/acado/lib" )
SET( ACADO_EXTERNAL_PACKAGES_DIR "~/catkin_ws/devel/share/acado/external_packages" )
SET( ACADO_CMAKE_DIR "~/catkin_ws/devel/share/acado/cmake" )

#
# List of ACADO static libraries
#
SET( ACADO_STATIC_LIBRARIES acado_toolkit)
#
# List of ACADO shared libraries
#
SET( ACADO_SHARED_LIBRARIES libacado_toolkit_s.so; libacado_casadi.a; libacado_csparse.a; libacado_qpoases.a )

#
# ACADO is shipped with embedded version of qpOASES. Here is specified
# where source and header files reside
#
SET( ACADO_QPOASES_EMBEDDED_SOURCES ~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/Bounds.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/CyclingManager.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/MessageHandling.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/QProblem.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/Utils.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/Constraints.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/Indexlist.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/QProblemB.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/SubjectTo.cpp;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC/EXTRAS/SolutionAnalysis.cpp )
SET( ACADO_QPOASES_EMBEDDED_INC_DIRS ~/catkin_ws/devel/share/acado/external_packages/qpoases/;~/catkin_ws/devel/share/acado/external_packages/qpoases/INCLUDE;~/catkin_ws/devel/share/acado/external_packages/qpoases/SRC )
